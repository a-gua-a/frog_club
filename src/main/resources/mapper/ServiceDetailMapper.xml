<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.frog.server.mapper.ServiceDetailMapper">
    <!-- 批量插入服务详情 -->
    <insert id="insertBatch" parameterType="com.frog.pojo.entity.ServiceDetail">
        insert into service_detail (service_id, name, value)
        values
        <foreach collection="serviceDetails" item="sd" separator=",">
            (#{sd.serviceId}, #{sd.name}, #{sd.value})
        </foreach>
    </insert>
</mapper>